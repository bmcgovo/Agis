# .env.development

# Application
NODE_ENV=development
PORT=3000

# Database
DB_HOST=localhost
DB_PORT=5432
DB_NAME=agis_dev
DB_USER=agis_dev
DB_PASSWORD=devpassword
DB_SSL=false

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Security
JWT_SECRET=dev_jwt_secret
SESSION_TIMEOUT=3600  # 1 hour (in seconds)

# Logging & Monitoring
LOG_LEVEL=debug
LOGTAIL_KEY=

# Compliance & Access
ENABLE_AUDIT_LOGGING=true
RBAC_ENABLED=true

# Accessibility & Compliance
WCAG_LEVEL=AA

# Email (for notifications, password resets, etc.)
EMAIL_HOST=localhost
EMAIL_PORT=1025
EMAIL_USER=dev@agency.gov.au
EMAIL_PASSWORD=devpassword

# Other
API_RATE_LIMIT=1000
